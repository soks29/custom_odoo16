<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Template for home and contactus -->
    <template id="example1" name="Home">
        <t t-call="website.layout">
            <div class="container">
                <h1>Hello KZM</h1>
                <table>
                    <thead>
                        <tr>
                            Name
                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="instance_r" t-as="insta">
                            <t t-sec="insta.name"/>
                        </t>
                    </tbody>
                </table>
            </div>
        </t>
    </template>
